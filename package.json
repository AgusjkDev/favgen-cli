{
    "name": "favgen-cli",
    "version": "1.0.0",
    "description": "CLI for generating favicon images and configs.",
    "keywords": [
        "favicon",
        "cli",
        "seo",
        "browserconfig",
        "manifest",
        "webmanifest",
        "pwa"
    ],
    "author": {
        "name": "Agust√≠n Arnoldi",
        "email": "agusjkdev7@gmail.com",
        "url": "https://github.com/agusjkdev"
    },
    "repository": {
        "url": "https://github.com/agusjkdev/favgen-cli"
    },
    "type": "module",
    "bin": {
        "favgen-cli": "dist/index.js"
    },
    "scripts": {
        "dev": "bun --watch src/index.ts",
        "build": "bun build src/index.ts --minify --target=node --outfile=dist/index.js",
        "start": "dist/index.js",
        "release": "bun run build && changeset publish",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@changesets/cli": "2.27.1",
        "@ianvs/prettier-plugin-sort-imports": "4.2.1",
        "@types/bun": "latest",
        "@types/to-ico": "1.1.3",
        "prettier": "3.2.5"
    },
    "peerDependencies": {
        "typescript": "5.0.0"
    },
    "dependencies": {
        "@clack/prompts": "0.7.0",
        "archiver": "7.0.1",
        "picocolors": "1.0.0",
        "png-to-ico": "2.1.8",
        "sharp": "0.33.3",
        "svgo": "3.2.0"
    }
}
